<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
    <navigation-rule>
        <from-view-id>/publico/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>sucesso</from-outcome>
            <to-view-id>/comum/homepage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>falha</from-outcome>
            <to-view-id>/publico/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/publico/cadastroServidor.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>sucesso</from-outcome>
            <to-view-id>/comum/homepage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>falha</from-outcome>
            <to-view-id>/publico/cadastroServidor.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
</faces-config>